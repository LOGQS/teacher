{
  "name": "ai-teacher",
  "version": "1.0.0",
  "description": "AI-powered educational presentation system",
  "scripts": {
    "start": "concurrently -n \"backend,frontend\" -c \"cyan,yellow\" \"python app.py\" \"npm run frontend\"",
    "frontend": "cd frontend && npm run dev",
    "backend": "python app.py",
    "dev": "npm run start",
    "install-deps": "npm install && cd frontend && npm install"
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}
